describe <%= class_node.name %>, '#<%= method_node.name %>' do
  it 'returns <%= method_node.return_name.gsub("'", '"') %>' do
    <%= class_node.instance_name %> = <%= class_node.name %>.new

    <%= class_node.instance_name %>.<%= method_node.method_call %>.should == <%= method_node.return_value %>
  end
end
